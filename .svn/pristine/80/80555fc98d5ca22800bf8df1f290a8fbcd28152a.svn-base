.tabbar {
    width: 100%;
    height: 98rpx;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    background-color: #F9F9F9;
    box-shadow: 0px 0px 0px 0px rgba(0, 0, 0, 0.3);
    border-top: 1px solid #D8D8D8;
    z-index: 100;
}

.list {
    width: 100%;
    height: 98rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    flex-wrap: nowrap;
}

.one {
    color: #ADADAD;
    text-align: center;
}

.one button {
    padding: 0;
    margin: 0;
    font-size: 20rpx;
    line-height: 30rpx;
    color: #ADADAD;
}

.one image {
    width: 44rpx;
    height: 44rpx;
}

.one .text {
    font-weight: 700;
    font-size: 20rpx;
    line-height: 30rpx;
}

.one .checked {
    color: #6317A5;
}