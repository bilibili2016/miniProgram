// components/common/buyPop/buyPop.js
Component({
    /**
     * 组件的属性列表
     */
    properties: {
        buyData: Object
    },

    /**
     * 组件的初始数据
     */
    data: {

    },

    /**
     * 组件的方法列表
     */
    methods: {
        close: function () {
            // 使用 triggerEvent 方法触发自定义组件事件，指定事件名、detail对象和事件选项
            this.triggerEvent('close')
        },
        goBuy: function (param) {
            this.triggerEvent('goBuy')
        }
    }
})