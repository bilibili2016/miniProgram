<!-- 标签列表 -->
<view class="labelList">
  <view class="item" wx:for="{{tagList}}" wx:key="{{index}}">
    <view class="title">{{item.tag_name}}</view>
    <view class="itemCon">
      <view class="tag {{children.checked?'checked':''}}" wx:for="{{item.children}}" wx:key="{{childIndex}}" wx:for-index="childIndex" wx:for-item="children" data-index="{{index}}" data-childIndex="{{childIndex}}" data-checked="{{children.checked}}" bindtap="handleTagClick">
        {{children.tag_name}}
      </view>
    </view>
  </view>
</view>