page {
  min-height: 100%;
  background-color: #f5f5f5;
}

.bottom .list {
  /* width: 100%; */
  background-color: #fff;
  padding: 40rpx 32rpx 0;
}

.list .inner {
  display: flex;
  flex-wrap: wrap;
  justify-content: flex-start;
  align-content: space-between;
  border-bottom: 1rpx solid #E0E0E1;
  background-color: #fff;
}

.bottom .inner .item {
  padding: 10rpx 35rpx 15rpx 35rpx;
  background-color: #FBF6FF;
  border: 1rpx solid #6317A5;
  border-radius: 44rpx;
  margin-bottom: 30rpx;
  margin-left: 20rpx;
}

.bottom .inner .item text {
  display: inline-block;
  vertical-align: middle;
  font-size: 28rpx;
  color: #6317A5;
}


.bottom .inner .item .leftIcon {
  display: inline-block;
  vertical-align: middle;
  width: 30rpx;
  height: 30rpx;
  margin-right: 12rpx;
}


.bottom .rightIcon {
  padding: 0 32rpx;
  position: relative;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #303030;
  background-color: #fff;
}

.bottom .rightIcon text {
  display: inline-block;
  vertical-align: middle;
}

.bottom .rightIcon .icon {
  display: inline-block;
  vertical-align: middle;
  position: absolute;
  right: 32rpx;
  top: 3rpx;
  font-size: 30rpx;
  font-weight: 900;
  color: #C7C8CC;
}

.btnCon {
  position: absolute;
  bottom: 0;
  left: 30rpx;
}

.btnCon .desc {
  text-align: center;
  font-size: 24rpx;
  color: #87868B;
  margin-bottom: 30rpx;
}

.bottom .btn {
  width: 686rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #6317A5;
  margin: 30rpx auto 50rpx;
  text-align: center;
  font-size: 30rpx;
  color: #fff;
  border-radius: 10rpx;
}