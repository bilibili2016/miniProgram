function install(req, request) {
    req.consult = {
        // 标签列表
        tagList(data) {
            return request({
                url: `${req.apiUrl}/Miniprogram/User/tagList`,
                method: 'GET',
                data,
            }, false);
        },
        // 选择标签
        setTags(data) {
            return request({
                url: `${req.apiUrl}/Miniprogram/User/setTags`,
                method: 'POST',
                data,
            }, false);
        }
    };
}

module.exports = {
    install,
};