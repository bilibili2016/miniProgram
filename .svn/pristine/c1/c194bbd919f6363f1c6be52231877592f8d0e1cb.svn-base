/* miniprogram/pages/user/setUser/common/common.wxss */
page {
  width: 100%;
  height: 100%;
}

.common {
  height: 100%;
  background-color: #f5f5f5;
  padding-top: 20rpx;
  box-sizing: border-box;

}

.con {
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 32rpx;
  background-color: #fff;
}

.con input {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #303030;
}
 .btn {
  width: 686rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #6317A5;
  text-align: center;
  font-size: 32rpx;
  color: #fff;
  border-radius: 10rpx;
  margin: 50rpx auto;
}