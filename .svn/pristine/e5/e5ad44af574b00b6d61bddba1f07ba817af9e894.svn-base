<!-- 标签设置 -->
<view class="user">
  <view class="bottom">
    <view class="list">
      <text class="item" wx:for="{{itemList}}" wx:key="{{index}}">
        <van-icon class="leftIcon" name="notes-o" />
        {{item.tag_name}}
      </text>
    </view>
    <view class="rightIcon" data-url="/pages/user/beTeacher/label/list/list" bindtap="handleLink">
      <van-icon class="icon" name="arrow" />
    </view>
    <view class="btnCon">
      <view class="desc">标签设置可帮助用户更好的找到</view>
      <view class="btn">保存</view>
    </view>
  </view>
</view>