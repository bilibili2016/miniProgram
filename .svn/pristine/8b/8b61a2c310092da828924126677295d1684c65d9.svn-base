<view class="home">
    <swiper indicator-dots="true" indicator-color="rgba(255,255,255,0.5)" indicator-active-color="#fff" autoplay="true" interval="3000" duration="500" circular="true">
        <block wx:for="{{bannerList}}">
            <swiper-item>
                <image src="{{item}}" class="slideImage" />
            </swiper-item>
        </block>
    </swiper>
</view>