page {
  min-height: 100%;
  background-color: #f5f5f5;
}


.bottom .item {
  position: relative;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;

  box-sizing: border-box;
}

.bottom .item .itemCon {
  width: 686rpx;
  height: 98rpx;
  position: relative;
  margin: 0 50rpx 0 30rpx;
  border-bottom: 1rpx solid #E0E0E1;
}

.bottom .item .leftIcon {
  position: absolute;
  left: 0;
  top: 10rpx;

}

.bottom .item .inner {
  font-size: 30rpx;
  color: #303030;
  margin-left: 60rpx;
}

.bottom .item .rightIcon {
  position: absolute;
  top: 0;
  right: 0;
}

.bottom .item .rightIcon .icon {
  /* display: inline-block;
    vertical-align: middle; */
  font-size: 30rpx;
  font-weight: 900;
  color: #C7C8CC;
}

.bottom .item .alert {
  display: inline-block;
  vertical-align: middle;
  width: 38rpx;
  height: 28rpx;
  text-align: center;
  line-height: 28rpx;
  background-color: #FF4A3A;
  color: #fff;
  font-size: 24rpx;
  border-radius: 16rpx;
  margin-right: 30rpx;
  margin-bottom: 11rpx;
}

.bottom .item.marginBox {
  margin-bottom: 20rpx;
}

.bottom .item.marginBox .itemCon {
  border: none !important;

}

.bottom .list .item:last-child .itemCon {
  border: none !important;
}

.bottom .btn {
  width: 686rpx;
  height: 88rpx;
  line-height: 88rpx;
  background-color: #fff;
  margin: 30rpx auto 50rpx;
  text-align: center;
  font-size: 30rpx;
  color: #6317A5;
}