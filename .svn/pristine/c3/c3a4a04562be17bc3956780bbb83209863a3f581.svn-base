/**app.wxss**/
.fl {
    float: left;
}

.fr {
    float: right;
}

.clearfix {
    overflow: auto;
    _height: 1%;
}

.container {
    display: flex;
    flex-direction: column;
    align-items: center;
    box-sizing: border-box;
}


button {
    background: initial;
}

button:focus {
    outline: 0;
}

button::after {
    border: none;
}