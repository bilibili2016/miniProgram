/* pages/home/activity/success/success.wxss */
.success {
  width: 100%;
  text-align: center;
}

.success image {
  width: 188rpx;
  margin-top: 135rpx;
}

.title {
  font-size: 50rpx;
  color: #303030;
  font-weight: bold;
  margin: 50rpx auto 30rpx;
}

.desc {
  display: inline-block;
  width: 530rpx;
  margin: 0 auto;
  font-size: 30rpx;
  color: #87868B;
  line-height: 42rpx;
}

.btn {
  margin: 0 auto;
  width: 300rpx;
  height: 78rpx;
  line-height: 78rpx;
  border-radius: 44rpx;
  border: 2px solid #6317A5;
  text-align: center;
  color: #6317A5;
